<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Setting.SettingScreenPersonalInformationBottomSheetFragment"
    android:orientation="vertical">

    <Button
        android:id="@+id/SettingPersonalInformationBottomSheetFragmentLibraryButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        app:layout_constraintBottom_toTopOf="@id/SettingPersonalInformationBottomSheetFragmentCancelButton"
        android:text="Library"/>

    <Button
        android:id="@+id/SettingPersonalInformationBottomSheetFragmentCameraButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        app:layout_constraintBottom_toTopOf="@id/SettingPersonalInformationBottomSheetFragmentLibraryButton"
        android:text="Camera"/>

    <Button
        android:id="@+id/SettingPersonalInformationBottomSheetFragmentCancelButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:text="Cancel"/>

</androidx.constraintlayout.widget.ConstraintLayout>